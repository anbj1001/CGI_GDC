                    <Label Content="Till" HorizontalAlignment="Left" Margin="448,27,0,0" VerticalAlignment="Top"/>
                    <Label Content="Incheckning" HorizontalAlignment="Left" Margin="191,52,0,0" VerticalAlignment="Top"/>
